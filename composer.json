{
    "name": "fresh/datetime",
    "description": "PHP library with additional functions for processing dates & times.",
    "keywords": ["datetime", "date", "time", "timezone", "midnight"],
    "homepage": "https://github.com/fre5h/datetime-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Henvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/datetime-php/issues"
    },
    "require": {
        "php": ">=7.3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpunit/phpunit": "^9.2",
        "slam/phpstan-extensions": "^5.0",
        "squizlabs/php_codesniffer": "^3.5",
        "thecodingmachine/phpstan-strict-rules": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\DateTime\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fresh\\DateTime\\Tests\\": "tests/"
        }
    }
}
