{
    "name": "fresh/datetime",
    "description": "PHP library that provides additional functions for processing dates & times.",
    "keywords": ["datetime", "date", "time", "timezone", "midnight"],
    "homepage": "https://github.com/fre5h/datetime-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Artem Henvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/datetime-php/issues"
    },
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "slam/phpstan-extensions": "^6.0",
        "squizlabs/php_codesniffer": "^3.6",
        "thecodingmachine/phpstan-strict-rules": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\DateTime\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fresh\\DateTime\\Tests\\": "tests/"
        }
    }
}
